/** recupera os parametros do MSN ***/
var oob = "";
try{
var ss = document.getElementsByTagName('script');
  for(var x = 0; x < ss.length; x++ ) {
    if( typeof ss[x].src !== "undefined" && ss[x].src.indexOf('dynad.net') > -1 && ss[x].src.indexOf('dc='+placementId+';') > -1 && typeof ss[x].getAttribute('OOBClickTrack') !== "undefined") { 
      oob = encodeURIComponent(ss[x].getAttribute('OOBClickTrack'));
      break;
    }
  }
}catch(e){}

/** carrega os dados do produto e dispara o callback com a flag correspondente ***/
var loadRTData = function(idrule, skus, callback) {
  loader("//sna.dynad.net/eval/" + idrule + "?p=" + skus + "&nivel_funil=produto&oob=" + oob, function(e) {
    callback((e != ""), e);
  }, function() {
    callback(false);
  });
}

/** aplica regra inicial dependendo do nivel de interacao do visitante ***/
var applyRules = function() {
  if (window[placementId + '_dft_recencia'] > -1 && window[placementId + '_dft_recencia'] < 24) { // carrinho de compras a 1 dia
    applyCart24Hours();
  } else if (window[placementId + '_dft_recencia'] > -1 && window[placementId + '_dft_recencia'] < (24 * 3)) { // carrinho de compras a 2 dias ou menos
    applyCart3Days();
  } else if (window[placementId + '_dft_recencia'] > -1 && window[placementId + '_dft_recencia'] < (24 * 7)) { // carrinho de compras a 7 dias ou menos
    applyCart7Days();
  } else if (window[placementId + '_dft_produtos'] != '') { // produtos da dafiti
    applyProductDafitiBR();
  } else if (window[placementId + '_dft_categorias'] != '') { //categoria da dafiti
    applyCategoryDafitiBR();
  } else if (window[placementId + '_kanui'] != '') { //qualquer nivel do funil para a kanui
    applyRTKanui();
  } else if (window[placementId + '_tricae'] != '') { //qualquer nivel do funil para a tricae
    applyRTTricae();
  } else { //nenhum historico encontrado.
    applyAmostraSemRT();
  }
}

var applyCart24Hours = function() {
  loadRTData(47, window[placementId + '_dft_carrinho'], function(f, e) {
    if (f) nextStage("_carrinho_1d_" + window[placementId + 'posCompra'], e, true);
    else applyProductDafitiBR();
  });
}

var applyCart3Days = function() {
  loadRTData(47, window[placementId + '_dft_carrinho'], function(f, e) {
    if (f) nextStage("_carrinho_3d_" + window[placementId + 'posCompra'], e, true);
    else applyProductDafitiBR();
  });
}

var applyCart7Days = function() {
  loadRTData(47, window[placementId + '_dft_carrinho'], function(f, e) {
    if (f) nextStage("_carrinho_7d_" + window[placementId + 'posCompra'], e, true);
    else applyProductDafitiBR();
  });
}

var applyProductDafitiBR = function() {
  if (window[placementId + '_dft_produtos'] != '') {
    loadRTData(47, window[placementId + '_dft_produtos'], function(f, e) {
      if (f) nextStage("_20150824_produto_dafiti_" + window[placementId + 'posCompra'], e, true);
      else applyCategoryDafitiBR();
    });
  } else applyCategoryDafitiBR();
}

var applyCategoryDafitiBR = function() {
  if (window[placementId + '_dft_categorias'] != '') {
    loadRTData(47, window[placementId + '_dft_categorias'], function(f, e) {
      if (f) nextStage("_20150824_categoria_dafiti_" + window[placementId + 'posCompra'], e, true);
      else applyRTKanui();
    });
  } else applyRTKanui();
}

var applyRTKanui = function() {
  /***
  if (window[placementId + '_kanui'] != '') {
    loadRTData(44, window[placementId + '_kanui'], function(f, e) {
      if (f) nextStage("_20150824_rt_loja_kanui_;rocket-sentinel-cid=13;", e, true);
      else applyRTTricae();
    });
  } else applyRTTricae();
  ***/
  applyRTTricae();
}

var applyRTTricae = function() {
  if (window[placementId + '_tricae'] != '') {
    loadRTData(45, window[placementId + '_tricae'], function(f, e) {
      if (f) nextStage("_20150824_rt_loja_tricae_;rocket-sentinel-cid=14;" + window[placementId + 'posCompra'], e, true);
      else applyAmostraSemRT();
    });
  } else applyAmostraSemRT();
}

var applyAmostraSemRT = function() {
  nextStage(window[placementId + 'posCompra'], '', false);
  //  if (Math.random() <= 0.25)
  //    nextStage("_20150824_amostra_tricae_fem_", "", false); 
  //  else
  //    nextStage("_20150824_amostra_dafiti_fem_", "", false);      
}

var getCompras = function(value){
  try {    
    var vendidos = JSON.parse(value);
    if( vendidos && vendidos.length > 0 ) {
        var ultimaCompra = new Date(Number(vendidos[0].t + '000')).getTime();
        var pos7dias = new Date(); pos7dias.setDate(pos7dias.getDate() - 7);  pos7dias=pos7dias.getTime();
        var pos30dias = new Date(); pos30dias.setDate(pos30dias.getDate() - 30); pos30dias=pos30dias.getTime();
        var pos60dias = new Date(); pos60dias.setDate(pos60dias.getDate() - 60); pos60dias=pos60dias.getTime();
    
        if (ultimaCompra < pos7dias && ultimaCompra > pos30dias) {
          window[placementId + 'posCompra'] = ';poscompra=7_30;';
          //7 a 30 dias
        } else if (ultimaCompra < pos30dias && ultimaCompra > pos60dias) {
          window[placementId + 'posCompra'] = ';poscompra=31_60;';
          //31 a 60 dias
        } else if (ultimaCompra < pos60dias) {
          window[placementId + 'posCompra'] = ';poscompra=61;';
          //60 dias+
        }
    }
  } catch(e){ 
      if( console && console.error )
        console.error('failed to check previous sales.', e);
  }      
}

var getRTLoja = function(db, cid, callback) {
  db.getItem("d" + cid + "s", function(k, v) {        
    if(v) {
      getCompras(v); 
      //recupera a data da ultima compra
    }
    db.getItem("d" + cid + "C", function(k, v) {
      //recupera dados do carrinho (dafiti)
      var v = null == v || "" == v ? {} : JSON.parse(v);
      if (v.length > 0) {
        var _arrSKUs = new Array();
        for (var x in v) if (v[x].v != '') _arrSKUs.push(v[x].v);
        if (_arrSKUs.length > 0) {
          callback(_arrSKUs.toString());
          return;
        }
      }

      db.getItem("d" + cid + "p", function(k, v) {
        //recupera dados do produto (dafiti)
        var v = null == v || "" == v ? {} : JSON.parse(v);
        if (v.length > 0) {
          var _arrSKUs = new Array();
          for (var x in v) if (v[x].v != '') _arrSKUs.push(v[x].v);
          if (_arrSKUs.length > 0) {
            callback(_arrSKUs.toString());
            return;
          }
        }

        db.getItem("d" + cid + "c", function(k, v) {
          //recupera dados de categoria (dafiti)
          var v = null == v || "" == v ? {} : JSON.parse(v);
          if (v.length > 0) {
            var _arrSKUs = new Array();
            for (var x in v) if (v[x].v != '') _arrSKUs.push(v[x].v);
            if (_arrSKUs.length > 0) {
              callback(_arrSKUs.toString());
              return;
            }
          }
          callback('');
        });
      });
    });
  });  
}

try {

  var recencia_carrinho = -1;
  new DADB(function(db) {
    db.getItem("d" + customerId + "s", function(k, v) {
      if(v) {
        getCompras(v); 
        //recupera a data da ultima compra
      }
      db.getItem("d" + customerId + "C", function(k, v) {
        //recupera dados do carrinho (dafiti)
        var v = null == v || "" == v ? {} : JSON.parse(v);
        if (v.length > 0) {
          window[placementId + '_dft_recencia'] = Math.round((new Date().getTime() / 1000 - parseInt(v[0].t)) / 3600);
          var _arrSKUs = new Array();
          for (var x in v) if (v[x].v != '') _arrSKUs.push(v[x].v);
          window[placementId + '_dft_carrinho'] = _arrSKUs.toString();
        }

        db.getItem("d" + customerId + "p", function(k, v) {
          //recupera dados do produto (dafiti)
          var v = null == v || "" == v ? {} : JSON.parse(v);
          if (v.length > 0) {
            var _arrSKUs = new Array();
            for (var x in v) if (v[x].v != '') _arrSKUs.push(v[x].v);
            window[placementId + '_dft_produtos'] = _arrSKUs.toString();
          }

          db.getItem("d" + customerId + "c", function(k, v) {
            //recupera dados de categoria (dafiti)
            var v = null == v || "" == v ? {} : JSON.parse(v);
            if (v.length > 0) {
              var _arrSKUs = new Array();
              for (var x in v) if (v[x].v != '') _arrSKUs.push(v[x].v);
              window[placementId + '_dft_categorias'] = _arrSKUs.toString();
            }

            getRTLoja(db, '13', function(skus) {
              window[placementId + '_kanui'] = skus;
              getRTLoja(db, '14', function(skus) {
                window[placementId + '_tricae'] = skus;
                applyRules();
              });
            });

          });
        });
      });

    });    

  });

